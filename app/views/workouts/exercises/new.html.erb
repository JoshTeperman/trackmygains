<%= form_with(model: BusinessModels::Workouts::Exercise, url: workout_exercises_path) do |f| %>
  <%= f.label :exercise_type_id %>
  <%= f.collection_select :exercise_type_id, ExerciseType.all, :id, :name, include_blank: 'Add exercise...' %>
  <p>search or type to add a new exercise to this workout</p>
  <%= f.submit 'Add Exercise' %>
<% end %>

